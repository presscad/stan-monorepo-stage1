.PRECIOUS: stanc$(EXE)

bin/stanc$(EXE) : src/stan/command/stanc.cpp $(TEMPLATE_INSTANTIATION)
	@echo 'here'
	@echo $(TEMPLATE_INSTANTIATION)
#	@mkdir -p $(dir $@)
#	$(CC) $(CFLAGS) $(CPPFLAGS) $< $(LDFLAGS) $(TARGET_ARCH) $(OUTPUT_OPTION)

bin/%.o : src/%.cpp
	@echo 'here'
	@mkdir -p $(dir $@)
	$(COMPILE.c) $(OUTPUT_OPTION) $<

TEMPLATE_INSTANTIATION := $(shell find src/stan -type f -name '*_inst.cpp')
TEMPLATE_INSTANTIATION := $(TEMPLATE_INSTANTIATION:src/%.cpp=bin/%.o)

